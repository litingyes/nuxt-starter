{
  "name": "nuxt-starter",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "description": "A nuxt project startup template",
  "author": "liting-yes <luz.liting@gmail.com> (https://me.litingyes.top/)",
  "license": "MIT",
  "homepage": "https://github.com/litingyes/nuxt-starter#readme",
  "bugs": {
    "url": "https://github.com/litingyes/nuxt-starter/issues"
  },
  "keywords": [
    "nuxt",
    "template",
    "starter"
  ],
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "commit": "git-cz",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/icon": "1.15.0",
    "@nuxt/ui": "^3.3.0",
    "@vueuse/core": "^13.6.0",
    "@vueuse/nuxt": "^13.6.0",
    "nuxt": "^4.0.2",
    "nuxt-lodash": "^2.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@types/node": "^22.17.0",
    "commitizen": "^4.3.1",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "inquirer": "^12.9.0",
    "lint-staged": "^13.3.0",
    "sass": "^1.89.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
